        "from pyngrok import ngrok\n",
        "\n",
        "# Set token\n",
        "ngrok_token = input(\"Enter your ngrok token: \").strip()\n",
        "ngrok.set_auth_token(ngrok_token)\n",
        "\n",
        "# Disconnect old tunnels\n",
        "for t in ngrok.get_tunnels():\n",
        "    try:\n",
        "        ngrok.disconnect(t.public_url)\n",
        "    except Exception:\n",
        "        pass\n",
        "\n",
        "# Start a fresh tunnel (use port 5050 if 5000 is busy)\n",
        "port = 5000\n",
        "tunnel = ngrok.connect(addr=port, proto=\"http\")\n",
        "print(\"‚úÖ Flask is public at:\", tunnel.public_url)\n",
        "print(f\"üåê Use: {tunnel.public_url}/chat or /ping\")\n",
        "\n",
